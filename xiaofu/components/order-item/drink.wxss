.ticket{ position: relative;}
.shadow-top::before{ content: ''; width: 100%; height: 6rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; top: -4rpx; left: 0;}
.ticket::after{ content: ''; width: 100%; height: 6rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; bottom: -4rpx; left: 0;}
.ticket .check-icon{ width: 120rpx; height: 120rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; top: 0; right: 0; display: none;}
.ticket-simple .check-icon{ display: block;}
.ticket-simple .shadow-frame{ box-shadow: 0 20rpx 30rpx rgba(0,0,0,0.2);}
.ticket .shadow-frame{ box-shadow: 0 50rpx 50rpx rgba(0,0,0,0.15);}
.ticket .top{ height: 320rpx; overflow: hidden; transition:height 0.5s cubic-bezier(.22,.62,.37,1);}
.ticket-simple .top{ height: 0;}
.ticket  .frame1{ width: 100%; height: 300rpx; background-color: #fff; display: flex; flex-direction: column; justify-content: center; align-items: center;}
.ticket  .check-frame1 { background-color: transparent;}
.ticket  .frame1 .line1{ width: 210rpx; height: 210rpx; margin-bottom: 10rpx; display: flex; justify-content: center; align-items: center; font-size: 70rpx; color: #878787;}
.ticket  .check-frame1 .line1 { color: #ffffff;}
.ticket  .frame1 .line1 .qrcode{ width: 100%; height: 100%;}
.ticket  .frame1 .ids{ font-size: 21rpx; letter-spacing: 6rpx; color: #000;}
.ticket .check-frame1 .ids{ color: #ffffff;}
.ticket  .line-top{ width: 100%; height: 10rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; background-color: rgba(0,0,0,0.07);}
.ticket  .line-bottom{ width: 100%; height: 11rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%;  background-color: rgba(0,0,0,0.07);}
.ticket  .line-bottom-check{ background-color: rgba(0,0,0,0); margin-top: 10rpx;}

.shadow-frame .title {
   font-size: 50rpx;
   font-family: 'Helve';
   padding:10rpx 20rpx;
   overflow: hidden;
   white-space: nowrap;
   text-overflow: ellipsis;
   border-bottom: 1px solid #f0f0f0;
   background-color: #ffffff;
}

.shadow-frame .address {
   font-size: 27rpx;
   overflow: hidden;
   white-space: nowrap;
   text-overflow: ellipsis;
   height: 74rpx;
   display: flex;
   align-items: center;
   padding: 0 20rpx;
   transition:height 0.3s cubic-bezier(.22,.62,.37,1);
   background-color: #ffffff;
   border-bottom: 1rpx solid #f0f0f0;
}
.shadow-frame .imgs{ width: 100%; height: 200rpx; overflow: hidden;}
.shadow-frame .imgs image{ width: 100%; height: 100%;}
.shadow-frame .user-info{ border-bottom: 1rpx solid #f0f0f0; display: flex; background-color: #ffffff;}
.shadow-frame .user-info .name{ width: 30%; box-sizing: border-box; border-right: 1rpx solid #f0f0f0; font-size: 36rpx; padding: 15rpx 20rpx;}
.shadow-frame .user-info .phone{ width: 70%; font-size: 36rpx; padding: 15rpx 20rpx; font-family: 'Helve';}

.shadow-frame .infos{ display: flex; background-color: #ffffff;}
.shadow-frame .infos .type{ width: 75%; box-sizing: border-box; border-right: 1rpx solid #f0f0f0; font-size: 66rpx; padding: 15rpx 20rpx;}
.shadow-frame .infos .index{ width: 25%; font-size: 70rpx; padding: 15rpx 20rpx; text-align: center; font-family: 'Helve';}
.shadow-frame .infos .index text{ font-size: 25rpx;}
