/* pages/detail/index.wxss */

@import "../template/template.wxss";

/* 秒杀代码 */

.numList {
    height: 120rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.numList .price {
    width: 490rpx;
    height: 120rpx;
    background: linear-gradient(90deg, rgba(232, 45, 76, 1) 0%, rgba(233, 101, 60, 1) 100%);
    padding-left: 30rpx;
}

.numList .price text,
.numList .activity text {
    display: block
}

.numList .price .p1 {
    font-size: 50rpx;
    font-family: PingFang-SC-Bold;
    font-weight: bold;
    color: rgba(243, 243, 243, 1);
}

.numList .price .p2 {
    font-size: 30rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
    color: rgba(243, 243, 243, 1);
}

.numList .price .p21 {
    font-size: 30rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
    color: rgba(243, 243, 243, 1);
    text-decoration: line-through;
}

.numList .activity {
    width: 230rpx;
    height: 120rpx;
    background: #FDF0EC;
    text-align: center
}

.numList .activity .text {
    font-size: 24rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
    color: rgba(233, 68, 69, 1);
    margin-top: 20rpx;
}

.numList .activity .time {
    display: flex;
    align-items: center;
    color: rgba(233, 68, 69, 1);
    margin-left: 35rpx
}

.numList .activity .time text {
    width: 40rpx;
    height: 39rpx;
    line-height: 39rpx;
    background: rgba(233, 68, 69, 1);
    border-radius: 4rpx;
    font-size: 24rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
    color: #fff
}


/* 限时购代码 */

.numList1 {
    width: 690rpx;
    height: 160rpx;
    background: linear-gradient(90deg, rgba(232, 45, 76, 1) 0%, rgba(233, 101, 60, 1) 100%);
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 30rpx
}

.numList1 .price text,
.numList1 .activity text {
    display: block
}

.numList1 .price .p1 {
    font-size: 50rpx;
    font-family: PingFang-SC-Bold;
    font-weight: bold;
    color: rgba(243, 243, 243, 1);
}

.numList1 .price .p2 {
    font-size: 30rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
    text-decoration: line-through;
    color: rgba(243, 243, 243, 1);
}

.numList1 .activity .time {
    font-size: 26rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
    color: rgba(243, 243, 243, 1);
    text-align: right
}

.numList1 .activity .day {
    width: 320rpx;
    height: 49rpx;
    line-height: 49rpx;
    background: rgba(0, 0, 0, 0.2);
    border-radius: 4rpx;
    font-size: 24rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
    color: rgba(243, 243, 243, 1);
    margin-top: 10rpx;
    text-align: center
}

.play-image {
    width: 90rpx;
    height: 90rpx;
    display: block;
    position: absolute;
    left: 20rpx;
    bottom: 20rpx;
    z-index: 99;
}

.video-area video {
    width: 100%;
    height: 750rpx;
    display: block;
}

.bb1 {
    text-align: center;
}

.info .price,
.info .code,
.info .productSelect,
.info .name {
    font-size: 32rpx;
    text-align: left;
}
.info .btn-share {
   width: 44rpx;
}
.info .btn-share image {
   width: 100%;
}


   /* 拼团 */

.numListSpell {
    height: 100rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 30rpx;
    background-size: cover;
}

.spellBack {
    background-image:url('data:image/png;base64,') !important
}

.numListSpell .nowPrice {
    font-size: 56rpx;
    font-family: PingFang-SC-Bold;
    font-weight: bold;
    color: #FFF;
    margin-right: 26rpx;
}

.numListSpell .priceTip {
    font-size: 32rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
    margin-right: 2rpx;
}

.numListSpell .price {
    display: flex;
    height: 100rpx;
    align-items: center;
}

.numListSpell .time {
    font-size: 24rpx;
}

.numListSpell .rightPrice {
    height: 100rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    color: #FFF;
}

.numListSpell .oldPrice {
    font-size: 26rpx;
    text-decoration: line-through;
}

.numListSpell .saleNum {
    font-size: 24rpx;
    background: rgba(0, 0, 0, 0.2);
    border-radius: 4rpx;
    padding: 4rpx 7rpx;
}

.numListSpell .activity {
    display: flex;
    flex-direction: column;
    justify-content: space-evenly;
    text-align: center;
    height: 100%;
}

.numListSpell .activity .time text {
    margin-right: 9rpx;
    padding: 0 4rpx;
    background: #603411;
    color: #FFF;
    border-radius: 4rpx;
    font-size: 28rpx;
}

.product .name span.numSpell {
    padding: 5rpx 12rpx;
    background-color: #DE3031;
    color: #FFF;
}

.joinSpell {
    border-top: 10rpx solid #f3f3f3;
    border-bottom: 0;
}

.joinSpell .lInfo {
    border-left: 0;
}

.joinSpell .rInfo {
    display: flex;
    align-items: center;
    color: #999;
    font-size: 26rpx;
}

.spellList {
    display: flex;
    justify-content: space-between;
    padding: 0 30rpx;
    align-items: center;
}

.spellList-border {
    display:flex;
    justify-content:space-between;
    align-items: center;
    border-top:1rpx solid #f3f3f3;
    padding:40rpx 0;
    width:100%;
}

.spellListImage {
    display: flex;
    align-items: center;
}

.spellList image {
    margin-right: 26rpx;
    border-radius: 50%;
    width: 100rpx;
    height: 100rpx;
}

.spellListImage text {
    font-size: 30rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow-x: hidden;
    max-width: 160rpx;
}

.spellListRight {
    display: flex;
}

.spellListTime {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    color: #666;
    font-size: 28rpx;
    align-items: flex-end;
}

.spellListTime text {
    white-space: normal;
}

.spellListTime span {
    color: #E60012;
}

.spellListBtn {
    margin-left: 20rpx;
    padding: 18rpx 37rpx;
    color: #FFF;
    font-size: 34rpx;
    background-color: #E60012;
    border-radius: 4rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
}

.ptMask {
    position: fixed;
    overflow: hidden;
}

.ptDialog {
    width: 678rpx;
    position: fixed;
    top: 40rpx;
    left: 36rpx;
    border-radius: 16rpx;
}

.closeImg {
    width: 44rpx;
    height: 44rpx;
    position: absolute;
    right: -20rpx;
    top: -20rpx;
}

.ptTitle {
    height: 100rpx;
    font-size: 34rpx;
    line-height: 100rpx;
    text-align: center;
    font-weight: 500;
    border-bottom: 1rpx solid #E5E5E5;
}

.ptItem {
    display: flex;
    width: 100%;
    box-sizing: border-box;
    padding: 24rpx 20rpx;
}

.ptL {
    width: 100rpx;
}

.ptC {
    flex: 1;
    padding-left: 14rpx;
}

.ptR {
    width: 180rpx;
}

.ptLImg {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
}

.ptName {
    color: #333;
    font-size: 28rpx;
    margin-right: 26rpx;
}

.ptNum {
    color: #666;
    font-size: 24rpx;
}

.ptTime {
    color: #666;
    font-size: 26rpx;
    margin-top: 4rpx;
}

.toPt {
    width: 158rpx;
    height: 64rpx;
    background: rgba(230, 0, 18, 1);
    border: 1px solid rgba(254, 18, 18, 1);
    border-radius: 4rpx;
    color: #fff;
    font-size: 32rpx;
    line-height: 64rpx;
    text-align: center;
    margin-top: 16rpx;
}

.bLine {
    width: 638rpx;
    height: 1rpx;
    background-color: #e5e5e5;
    margin-left: 20rpx;
}

.ptFooter {
    height: 90rpx;
    line-height: 90rpx;
    font-size: 24rpx;
    color: #999;
    text-align: center;
    border-top: 1rpx solid #e5e5e5;
}

.toPtDialog {
    width: 678rpx;
    position: fixed;
    top: 240rpx;
    left: 36rpx;
    border-radius: 16rpx;
}

.joinPt {
    height: 100rpx;
    font-size: 34rpx;
    line-height: 120rpx;
    text-align: center;
    font-weight: 500;
}

.joinTime {
    font-size: 26rpx;
    color: #666;
    text-align: center;
    font-weight: 500;
    height: 26rpx;
    margin-top: -4rpx;
}

.personImgList {
    margin-top: 64rpx;
    text-align: center;
}

.personItem {
    display: inline-block;
    position: relative;
    margin: 0 20rpx;
}

.personImg {
    width: 114rpx;
    height: 114rpx;
    border-radius: 50%;
}

.pinzu {
    width: 58rpx;
    height: 30rpx;
    position: absolute;
    left: 10rpx;
}

.goPt {
    display: inline-block;
    width: 550rpx;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    background: rgba(222, 48, 49, 1);
    border: 1rpx solid rgba(254, 18, 18, 1);
    border-radius: 4rpx;
    font-size: 36rpx;
    color: #fff;
    margin-left: 64rpx;
    margin-top: 30rpx;
    margin-bottom: 50rpx;
}

.bottomTool .r.addCartSpellBtn {
    display: flex;
    flex-direction: column;
    padding: 9rpx 0;
}

.SelectLayer .pro .info .old-price {
    text-decoration: line-through;
    color: #999;
    font-size: 24rpx;
}

.SelectLayer .pro .info .old-price span {
    color: #999;
    font-size: 24rpx;
}
