page { background-color: #eef3f9;}

.medal-frame { width: 100%; background: url(data:image/png;base64,) no-repeat; background-size: 100%; overflow: hidden;}

.medal-list { display: flex; flex-wrap: wrap; margin: 60rpx 55rpx 0rpx 55rpx;}
.medal-list .medal-item { width: 160rpx; margin:0 0 60rpx 0; position: relative;}
.medal-list .medal-item-dot::after { content: ''; width: 10rpx; height: 10rpx; border-radius: 50%; background-color: #ff3a65; position: absolute; top: 0; right: 0;}
.medal-list .medal-item .medal-icon { width: 100%; height: 135rpx; margin-bottom: 15rpx;}
.medal-list .medal-item .gift-icon { width: 40rpx; position: absolute; top: -50rpx; right: 30rpx; animation: float 1s linear infinite;}
@keyframes float {
   0%,100% { transform: translate(0,0)}
   50% { transform: translate(0,10rpx)}
}
.medal-list .medal-item view { text-align: center; color: #848b91; font-size: 24rpx;}

.person-info { display: flex; justify-content: center; align-items: center; margin-top: 40rpx;}
.person-info .name { font-size: 24rpx; color: #1c1c1c; margin-left: 40rpx;}

.medal-frame .hint{ color: rgba(22, 160, 161, 1); font-size: 32rpx; text-align: center;}

.medal-img { width: 100%; display: block;}
