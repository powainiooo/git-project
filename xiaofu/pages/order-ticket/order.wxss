.title-en{ font-size: 100rpx; font-family: 'Helve'; line-height: 0.9; background-color: #ffffff; padding: 40rpx; border-bottom: 1rpx solid #e5e5e5;}
.title-cn{ font-size: 72rpx; font-weight: bold;  background-color: #ffffff; padding: 20rpx 40rpx; border-bottom: 1rpx solid #e5e5e5;}

.order-list{ margin: 80rpx 0;}
.order-list .order-item{ padding: 0 90rpx; height: 280rpx; overflow: hidden; position: relative; margin-bottom: 30rpx; transition: transform 0.15s cubic-bezier(.18,.47,.31,.96); padding-top: 10rpx;}
.order-list .order-item::after{ content: '';  height: 46rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; left: 30rpx; right: 30rpx; bottom: 0;}
.order-list .order-item-touch{ transform: scale(0.94,0.94);}

.detail-frame{ width: 100%; height: 280rpx; position: fixed; top: 150rpx; left: 0; overflow: hidden; box-sizing: border-box; padding: 10rpx 0;  transition:height 0.5s cubic-bezier(.22,.62,.37,1);}
.details-show{ position: absolute;}
.detail-frame .swiper-item{ padding: 0 20rpx; box-sizing: border-box; padding-top: 10rpx;}
.detail-frame .hide{ opacity: 0;}
.detail-frame .check-infos{ margin: 20rpx 90rpx; display: flex; justify-content: space-between;}
.detail-frame .check-infos view{ font-size: 28rpx;}
.detail-frame .check-infos view text{ font-size: 70rpx; font-family: 'Helve'; margin-left: 16rpx;}
.detail-frame .hint-section{ margin: 50rpx 90rpx; border-top: 1rpx solid #e2e2e2; padding-top: 50rpx;}