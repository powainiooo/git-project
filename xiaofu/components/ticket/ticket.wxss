.ticket-item { width: 750rpx; transition: transform 0.15s cubic-bezier(.18,.47,.31,.96);}
.ticket-item-tap { transform: scale(0.95)}
.ticket-frame { width: 750rpx; position: relative; transform-origin: 0 0; font-family: Helve; padding-top: 20rpx;}
/*.ticket-frame::before { content: ""; width: 100%; height: 20rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; left: 0; top: -18rpx;}*/
.ticket-frame::after { content: ""; width: 100%; height: 230rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; left: 0; bottom: -26rpx;}
.ticket-frame .ticket-top { width: 100%; display: block;}
.ticket-frame .top { height: 170rpx; box-sizing: border-box; border-bottom: 2rpx solid #e3e5e4; background-color: #ffffff; overflow: hidden;}
.ticket-frame .top .date { width: 310rpx; box-sizing: border-box; border-right: 2rpx solid #e3e5e4; float: left;}
.ticket-frame .top .date2 { width: 410rpx;}
.ticket-frame .top .date3 { width: 410rpx;}
.ticket-frame .top .date .year { height: 50rpx; display: flex; justify-content: space-between; align-items: center; padding: 0 20rpx; border-bottom: 2rpx solid #e3e5e4; font-size: 28rpx;}
.ticket-frame .top .date .days { font-size: 100rpx; height: 120rpx; padding: 0 0 0 20rpx; display: flex; align-items: center;}
.ticket-frame .top .date2 .days { font-size: 80rpx;}
.ticket-frame .top .date3 .days { font-size: 66rpx;}
.ticket-frame .top .logo { height: 100%; text-align: center; font-size: 0}
.ticket-frame .top .logo-img { width: 242rpx; height: 100%;}

.ticket-frame .title { font-size: 44rpx; line-height: 1.2; padding: 20rpx; overflow: hidden; word-break: break-all; background-color: #ffffff;}
.ticket-frame .time { font-size: 30rpx; overflow: hidden; white-space: nowrap; text-overflow: ellipsis; border-top: 1px solid #f0f0f0; height: 78rpx; display: flex; align-items: center; padding: 0 20rpx; background-color: #ffffff; transition:height 0.3s cubic-bezier(.22,.62,.37,1);}
.ticket-frame .address { font-size: 28rpx; overflow: hidden; height: 78rpx; display: flex; align-items: center; border-top: 1px solid #e3e5e4; padding: 0 20rpx; background-color: #ffffff; transition:height 0.3s cubic-bezier(.22,.62,.37,1);}

.ticket-frame .imgs-frame { width: 100%; height: 650rpx; transition:height 0.3s cubic-bezier(.22,.62,.37,1); overflow: hidden; display: flex; align-items: center; position: relative; mask-image: url(data:image/png;base64,); -webkit-mask-image: url(data:image/png;base64,); mask-size: 100%; -webkit-mask-size: 100%; mask-position: center bottom; -webkit-mask-position: center bottom;}
.ticket-frame .imgs { width: 100%; display: block;}

.ticket-item-fold .ticket-frame .title { word-break: break-all; white-space: nowrap; text-overflow: ellipsis;}
.ticket-item-fold .ticket-frame .time { height: 0;}
.ticket-item-fold .ticket-frame .address { height: 0;}
.ticket-item-fold .ticket-frame .imgs-frame { height: 350rpx;}
.ticket-item-fold .ticket-frame::after { height: 394rpx; background-image: url(data:image/png;base64,); bottom: -190rpx;}

.ticket-item-hideimg .ticket-frame .imgs-frame { height: 0;}
.ticket-item-hideimg .ticket-frame::after { height: 20rpx; bottom: -18rpx; background-image: url(data:image/png;base64,);}

.ticket-frame .sold-out:after{ content: ''; width: 100%; height: 100%; position: absolute; left: 0; top: 0; background: url(data:image/png;base64,) no-repeat; background-size: 150rpx auto; background-position: 20rpx center; background-color: rgba(0,0,0,0.5);}
