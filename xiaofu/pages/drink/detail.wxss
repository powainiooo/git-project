.buy-frame{ margin-bottom: 200rpx;}

.banner{ margin-top: 122rpx; height: 260rpx; position: relative; overflow: hidden;}
.banner::before{ content: ''; width: 100%; height: 46rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; top: -34rpx; left: 0;}
.banner image{ width: 100%; height: 100%;}

.title{ padding: 20rpx 30rpx; border-bottom: 1rpx solid #0d1025; font-size: 50rpx; font-family: 'Helve'; color: #ffffff}
.address{ padding: 20rpx 30rpx; border-bottom: 1rpx solid #0d1025; font-size: 27rpx; color: #ffffff}
.infos{ padding: 10rpx 0; margin: 0 30rpx; border-bottom: 1rpx solid #0d1025; display: flex; justify-content: space-between; align-items: center;}
.infos .price{ font-size: 100rpx; font-family: 'Helve'; color: #ffffff;}
.infos .price text{font-size: 33rpx;}
.infos .logo{ width: 90rpx; height: 90rpx; margin-right: 30rpx;}

#bodyFrame .hint-section { border-top: 1rpx solid #0d1025; padding-top: 50rpx; margin: 40rpx 30rpx; color: #ffffff}

.form-frame{ margin: 50rpx 30rpx 50rpx 30rpx;}
.form-frame .input{ width: 400rpx; height: 55rpx; border: 2rpx solid #000; font-size: 30rpx; border-radius: 8rpx; padding:0 20rpx; box-sizing: border-box; margin-bottom: 20rpx; background-color: #ffffff;}

.buy-frame .footer{ width: 100%; height: 160rpx; display: flex; position: fixed; left: 0; bottom: 0;z-index: 110;box-shadow: 0 -5rpx 10rpx rgba(0,0,0,0.1);}
.buy-frame .footer-shaodow{ box-shadow: none; }
.buy-frame .footer .ticket-type,.buy-frame .footer .ticket-number{padding-bottom: 10rpx; display: flex; justify-content: center; align-items: center; font-size: 24rpx;background-image: url("data:image/png;base64,"); background-repeat: no-repeat; background-size: 22rpx 13rpx;}
.buy-frame .footer .ticket-number{ width: 25%; background-color: #f5f5f5; font-size: 33rpx;  background-position: 78% 70rpx;align-items: stretch;}
.buy-frame .footer .ticket-number view{ width: 100rpx; height: 100%; display: flex; justify-content: center; align-items: center;}
.buy-frame .footer .ticket-number text{font-size: 24rpx;}
.buy-frame .footer .btn{ width: 75%; background-color: #002aac; padding-bottom: 10rpx; display: flex; justify-content: center; align-items: center; font-size: 34rpx; color: #fff;}
.buy-frame .footer .btn-disabled{ width: 36%; background-color: #b5b5b6;}
.buy-frame .footer .stop-sale{ width: 64%; justify-content: center; color: #888888; font-size: 24rpx; flex-direction: column; display: flex; background-color: #ffffff;}
.buy-frame .footer .stop-sale view{ margin-left: 70rpx;}
.buy-frame .footer .stop-sale view:first-child{ font-size: 34rpx; margin-top: -20rpx;}
