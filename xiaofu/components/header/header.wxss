 @import '../../res/css/icons.wxss';

.header{ width: 100%;position: fixed; top: 0; left: 0; z-index: 600;}
.header-shadow{ box-shadow: 0 7rpx 10rpx rgba(0,0,0,0.25);}
.header .opera-line{ height: 120rpx;  padding: 0 55rpx; background-color: #002bb0; display: flex;  align-items: center; justify-content: space-between; position: relative; z-index: 600;}
.header .logo{ width: 190rpx;}
.header .btns{ width: 160rpx; display: flex; flex-direction: row-reverse; justify-content: space-between; align-items: center;}
.header .btns .icons{ width: 80rpx; height: 80rpx; font-size: 40rpx; color: #fff; display: flex; justify-content: center; align-items: center;}
.header .btns .icons:first-child{justify-content: flex-end;}
.header .btns .icon-search{ width: 80rpx; height: 80rpx; background: url(data:image/png;base64,) no-repeat; background-size: 56%; background-position: center center;}
.header .btns .icon-xuanze { transform: scaleY(-1)}

.nav-frame{ width: 100%; height: calc(100% - 120rpx); background-color: #002bb0; position: fixed; top: -100%; left: 0; z-index: 450; overflow-y: scroll;}
.nav-frame .person-info{ margin: 0 15rpx; height: 220rpx; display: flex; align-items: center; border-bottom: 1rpx solid #b3ceff;}
.nav-frame .person-info .avatar{ width: 140rpx; height: 140rpx; border-radius: 8px; margin: 0 40rpx;}
.nav-frame .person-info .name{ font-size: 34rpx; color: #b3ceff;}
.nav-frame .nav-list{ margin:0 15rpx 300rpx 15rpx;}
.nav-frame .nav-list .nav-item{ height: 130rpx; display: flex; align-items: center; border-bottom: 1px solid #002684; padding-left: 40rpx;}
.nav-frame .nav-list .nav-item .name{ font-size: 34rpx; color: #b3ceff;}

.nav-order{ height: 160rpx; position: fixed; bottom: -300rpx; left: 10rpx; right: 10rpx; background: #fff; z-index: 550;}
.nav-order .menu-line{ width: 100%; position: absolute; top: -82rpx; left: 0;}
.nav-order .link{ text-align: center; margin-top: 40rpx;}
.nav-order .menu-title{ width: 150rpx;}

.bg-cover{ width: 100vw; height: 100vh; position: fixed; top: 0; left: 0; background-color: rgba(0,0,0,0.73); z-index: 850;}

.share-frame{ width: 100%; height: 330rpx; position: fixed; bottom: -330rpx; left: 0; z-index: 900; background-color: #ffffff;}
.share-frame .line1{ margin: 0 50rpx; height: 100rpx; display: flex; justify-content: space-between; align-items: center; font-size: 40rpx; border-bottom: 1rpx solid #dfdfdf; box-sizing: border-box;}
.share-frame .line2{ margin: 0 50rpx; height: 230rpx; display: flex; justify-content: space-around; align-items: center;}
 .share-frame .line2 .btn-share, .share-frame .line2 .btn-poster{ border: none; font-size: 24rpx; color: #888888; padding-top: 70rpx; background: url('data:image/png;base64,') no-repeat; background-position: center 0; background-size: 80rpx;}
 .share-frame .line2 .btn-poster{ background-image: url('data:image/png;base64,');}
















