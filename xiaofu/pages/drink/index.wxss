.address-select{ height: 100rpx; background-color: #002aac;background: repeating-linear-gradient(90deg,#09268b, #002bb3 50%, #09268b 100%); position: fixed; top: 120rpx; left: 0; z-index: 400}
.address-frame{ width: 100%; display: flex;}
.address-frame view{ width: 150rpx; height: 100rpx; display: flex; justify-content: center; align-items: center; font-size: 30rpx; color: #0092db;}
.address-frame .active{ font-size: 34rpx; color: #fff;}

.title-img{ width: 100%; margin-top: 100rpx;}

.item-list{ padding: 0 10rpx;}
.item-list .item{ width: 100%; height: 270rpx; display: flex; position: relative; background-color: #ffffff; margin-bottom: 40rpx;transition: transform 0.15s cubic-bezier(.18,.47,.31,.96);}
.item-list .item-touch{transform: scale(0.94,0.94);}
.item-list .item::before{ content: ''; width: 125rpx; height: 125rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; top: 0; left: -1rpx; z-index: 10;}
.item-list .item::after{ content: ''; width: 100%; height: 48rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; bottom: -42rpx; left: 0; z-index: 10;}
.item-list .item .img{ width: 65%; height: 100%;}
.item-list .item .infos{ width: 35%; height: 100%; display: flex; flex-direction: column;}
.item-list .item .infos .name{ margin:0 10rpx; padding: 20rpx 0; font-size: 27rpx; border-bottom: 1rpx solid #c0c0c0;}
.item-list .item .infos .logo{ display: flex; justify-content: center; align-items: center; height: 100%;}
.item-list .item .infos .logo image{ width: 90rpx; height: 90rpx;}
.item-list .item .sold-out{width: 65%; height: 100%; position: absolute; left: 0; top: 0; background: url(data:image/png;base64,) no-repeat; background-size: 100rpx auto; background-position: 20rpx 75%; background-color: rgba(0,0,0,0.5);}


.load-hint{ margin-bottom: 50rpx;  font-size: 24rpx; color: #888888; text-align: center;}

.result-frame{ margin:  0 50rpx 50rpx 50rpx;}
.result-frame .icons{ font-size: 200rpx; color: #002aac; display: block; margin-bottom: 60rpx;}
.result-frame .hint-en{ font-size: 100rpx; font-family: 'Helve'; line-height: 0.9;}
.result-frame .hint-cn{ font-size: 72rpx; margin-top: 60rpx;}