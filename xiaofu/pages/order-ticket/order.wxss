.title-en{ font-size: 100rpx; font-family: 'Helve'; line-height: 0.9; background-color: #ffffff; padding: 40rpx; border-bottom: 1rpx solid #e5e5e5;}
.title-cn{ font-size: 72rpx; font-weight: bold;  background-color: #ffffff; padding: 20rpx 40rpx; border-bottom: 1rpx solid #e5e5e5;}

.order-frame{ width: 200%; display: flex; transition: transform 0.5s ease-in-out;}
.order-frame-left{ transform:translateX(-50%);}
.order-list{ margin: 80rpx 0; width: 50%;}
.order-list .order-item{ padding: 0 90rpx; height: 245rpx; overflow: hidden; position: relative; margin-bottom: 30rpx; transition: transform 0.15s cubic-bezier(.18,.47,.31,.96); padding-top: 10rpx;}
.order-list .order-item::after{ content: '';  height: 46rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; left: 30rpx; right: 30rpx; bottom: 0;}
.order-list .order-item-touch{ transform: scale(0.94,0.94);}

.item-list{ padding: 0 10rpx; margin: 80rpx 0; width: 50%; box-sizing: border-box;}
.item-list .item{ width: 100%; height: 270rpx; display: flex; position: relative; background-color: #ffffff; margin-bottom: 40rpx;transition: transform 0.15s cubic-bezier(.18,.47,.31,.96);}
.item-list .item-touch{transform: scale(0.94,0.94);}
.item-list .item::before{ content: ''; width: 125rpx; height: 125rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; top: 0; left: 0;}
.item-list .item::after{ content: ''; width: 100%; height: 48rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; bottom: -42rpx; left: 0;}
.item-list .item .img{ width: 65%; height: 100%;}
.item-list .item .infos{ width: 35%; height: 100%;}
.item-list .item .infos .name{ margin:0 10rpx; padding: 20rpx 0; font-size: 27rpx; border-bottom: 1rpx solid #c0c0c0;}
.item-list .item .infos .logo{ display: flex; justify-content: center; align-items: center; margin-top: 40rpx;}
.item-list .item .infos .logo image{ width: 70rpx; height: 70rpx;}


.footer-tab{ width: 100%; height: 160rpx; background-color: #ffffff; display: flex; position: fixed; left: 0; bottom: 0; z-index: 500;box-shadow: 0 -30rpx 40rpx rgba(0,0,0,0.1);}
.footer-tab view{ box-sizing: border-box; flex: 1; padding-bottom: 30rpx; display: flex; justify-content: center; align-items: center; font-size: 34rpx;}
.footer-tab view.active{ background-color: #002aac; color: #fff;}
.order-body{ margin-bottom: 200rpx;}