<!-- 顶部导航 -->
<view class="header {{showShadow ? 'header-shadow' : ''}}">
  <view class="opera-line">
    <image src='../../res/images/logo.png' mode="widthFix" class="logo" bindtap='gotoHome'></image>
    <view class="btns">
       <view class="icons icon-xuanze" wx:if="{{showArrow}}" bindtap="tapArrow"></view>
       <view class="icons icon-guanbi" wx:if="{{showClose}}" bindtap="doClose"></view>
      <view class="icons icon-gengduo" wx:if="{{showMenu}}" bindtap="openMenu"></view>
      <view class="icon-search" wx:if="{{showRefresh}}" bindtap="doRefresh"></view>
      <view class="icons icon-fenxiang" wx:if="{{showShare}}"  data-val="{{true}}" bindtap="toggleShare"></view>
    </view>
  </view>
</view>

<!-- 更多菜单 -->
<view class='nav-frame' animation="{{navAniData}}">
  <view class='person-info' wx:if="{{userInfo !== null}}">
    <image src="{{userInfo.avatarUrl}}" class="avatar"></image>
    <text class='name'>{{userInfo.nickName}}</text>
  </view>
   <view class="vip-logo"
         hover-class="vip-logo-tap"
         hover-start-time="10"
         hover-stay-time="50"
         bindtap="toVip"
         wx:if="{{vipData.status === 1}}">
      <image src='../../res/images/vip-logo.png' mode="widthFix" class="icon"></image>
      <view class="time">{{vipData.data.vaild}} 到期</view>
   </view>
  <view class='person-info' wx:if="{{userInfo === null}}" bindtap="gotoLogin">
    <image src="../../res/images/default-avatar.jpg" class="avatar"></image>
    <text class='name'>点击登录账户</text>
  </view>
  <view class='nav-list'>
    <navigator wx:for="{{navList}}" wx:for-index="idx" wx:for-item="item" wx:key="idx" url="{{item.url}}" class='nav-item' hover-class='none' bindtap="doClose" open-type="{{item.type}}">
      <text class='name'>{{item.name}} | {{item.nameEn}}</text>
    </navigator>
  </view>
</view>

<!-- 订单中心 -->
<view class='nav-order' animation="{{orderAniData}}">
  <image mode='widthFix' src='data:image/png;base64,' class='menu-line'></image>
  <navigator class='link' bindtap="doClose" url="/pages/order-ticket/order" hover-class="none">
    <image mode='widthFix' src='data:image/png;base64,' class='menu-title'></image>
  </navigator>
</view>

<!--黑色遮罩-->
<view class="bg-cover" wx:if="{{showCover}}"></view>

<!--分享-->
<view class="share-frame" animation="{{shareAnimate}}">
  <view class="line1">
    <text>分享</text>
    <view class="icons icon-guanbi" data-val="{{false}}" bindtap="toggleShare"></view>
  </view>
  <view class="line2">
    <button class="btn-share" plain open-type="share">
      <text>转发给好友</text>
    </button>
    <button class="btn-poster" plain bindtap="drawPoster">
      <text>生成分享海报</text>
    </button>
  </view>
</view>
