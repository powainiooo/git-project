.ticket{ position: relative;}
.ticket-top::before{ content: ''; width: 100%; height: 10rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; top: -8rpx; left: 0;}
.ticket::after{ content: ''; width: 100%; height: 10rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; bottom: -8rpx; left: 0;}
.ticket .shadow-frame{ box-shadow: 0 20rpx 30rpx rgba(0,0,0,0.2);}
.ticket .top{ height: 320rpx; overflow: hidden; transition:height 0.5s cubic-bezier(.22,.62,.37,1);}
.ticket-simple .top{ height: 0;}
.ticket  .frame1{ width: 100%; height: 300rpx; background-color: #fff; display: flex; flex-direction: column; justify-content: center; align-items: center;}
.ticket  .frame1 .line1{ width: 210rpx; height: 210rpx; margin-bottom: 10rpx;}
.ticket  .frame1 .line1 .qrcode{ width: 100%; height: 100%;}
.ticket  .frame1 .ids{ font-size: 21rpx; letter-spacing: 6rpx;}
.ticket  .line-top{ width: 100%; height: 10rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%;}
.ticket  .line-bottom{ width: 100%; height: 11rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%;}

.shadow-frame .line2 {
    border-bottom: 1rpx solid #f0f0f0;
    display: flex;
    background-color: #ffffff;
}

.shadow-frame .line2 .date {
    border-right: 1px solid #f0f0f0;
    padding: 10rpx 0;
}

.shadow-frame .line2 .date .year {
    font-size: 27rpx;
    font-family: 'Helve';
    display: flex;
    justify-content: space-between;
    border-bottom: 1px solid #f0f0f0;
    padding: 0 20rpx 10rpx 20rpx;
}
.ticket-small .line2 .date .year{ font-size: 17rpx;}

.shadow-frame .line2 .date .day {
    font-size: 100rpx;
    font-family: 'Helve';
    padding: 0 20rpx;
}
.ticket-small .line2 .date .day{ font-size: 66rpx;}

.shadow-frame .line2 .logo {
    width: calc(100% - 200rpx);
    display: flex;
    justify-content: center;
    align-items: center;
}

.shadow-frame .line2 .logo .logo-img {
    width: 200rpx;
    max-height: 100rpx;
}

.shadow-frame .title {
    font-size: 50rpx;
    font-family: 'Helve';
    padding: 20rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    border-bottom: 1px solid #f0f0f0;
    background-color: #ffffff;
}

.shadow-frame .range {
    font-size: 30rpx;
    font-family: 'Helve';
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    border-bottom: 1px solid #f0f0f0;
    height: 78rpx;
    display: flex;
    align-items: center;
    padding: 0 20rpx;
    transition:height 0.3s cubic-bezier(.22,.62,.37,1);
    background-color: #ffffff;
}
.shadow-frame .address {
    font-size: 27rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    height: 74rpx;
    display: flex;
    align-items: center;
    padding: 0 20rpx;
    transition:height 0.3s cubic-bezier(.22,.62,.37,1);
    background-color: #ffffff;
    border-bottom: 1rpx solid #f0f0f0;
}
.shadow-frame .user-info{ border-bottom: 1rpx solid #f0f0f0; display: flex; background-color: #ffffff;}
.shadow-frame .user-info .name{ width: 30%; box-sizing: border-box; border-right: 1rpx solid #f0f0f0; font-size: 36rpx; padding: 15rpx 20rpx;}
.shadow-frame .user-info .phone{ width: 70%; font-size: 36rpx; padding: 15rpx 20rpx; font-family: 'Helve';}

.shadow-frame .infos{ display: flex; background-color: #ffffff;}
.shadow-frame .infos .type{ width: 75%; box-sizing: border-box; border-right: 1rpx solid #f0f0f0; font-size: 66rpx; padding: 15rpx 20rpx;}
.shadow-frame .infos .index{ width: 25%; font-size: 70rpx; padding: 15rpx 20rpx; text-align: center; font-family: 'Helve';}