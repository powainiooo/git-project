<!--index.wxml-->
<view class="container">
  <header-view></header-view>
  <view class='body'>
    <view class='sponsor-list'>
      <view wx:for="{{dataList}}"
            wx:for-index="index"
            wx:for-item="item"
            wx:key="{{index}}"
            data-item="{{item}}"
            bindtap="toIndex"
            hover-class='none'>
        <image src="{{imgSrc+item.logo_img}}" mode='aspectFit'></image>
      </view>
    </view>
  </view>
</view>
