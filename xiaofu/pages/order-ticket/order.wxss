@import '../../res/css/icons.wxss';
.title-frame { margin: 120rpx 0 80rpx 0;}
.bg-gradient2 { background: repeating-linear-gradient(200deg,#000000, #232961 50%, #232961 100%);}
.bg-white { background-color: #ffffff;}
.title-en{ font-size: 100rpx; font-family: 'Helve'; line-height: 0.9; padding: 40rpx; border-bottom: 1rpx solid #e5e5e5;}
.title-cn{ font-size: 72rpx; font-weight: bold; padding: 20rpx 40rpx; border-bottom: 1rpx solid #e5e5e5;}
.bg-gradient2 .title-en { border-bottom: 1rpx solid #0d1025; color: #ffffff;}
.bg-gradient2 .title-cn { border-bottom: 1rpx solid #0d1025; color: #ffffff;}

.order-frame{ width: 200%; display: flex; transition: transform 0.5s ease-in-out;}
.order-frame-left{ transform:translateX(-50%);}
.order-list, .item-list{ width: 50%; height: 100vh; box-sizing: border-box; padding-bottom: 160rpx;}
.order-list .order-item{ height: 245rpx; overflow: hidden; position: relative; margin-bottom: 30rpx; transition: transform 0.15s cubic-bezier(.18,.47,.31,.96); padding-top: 10rpx; transform-origin: 50% 100%;}
.order-list .order-item::after{ content: '';  height: 46rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; left: 30rpx; right: 30rpx; bottom: 0;}
.order-list .order-item>view { padding: 0 90rpx; height: 100%; overflow: hidden;}
.order-list .order-item-touch{ transform: scale(0.94);}

.item-list .item { position: relative; transition: transform 0.15s cubic-bezier(.18,.47,.31,.96); margin: 0 22rpx;}
.item-list .item-touch{transform: scale(0.94,0.94);}
.item-list .item::before { content: ''; width: 118rpx; height: 118rpx; background: url(data:image/png;base64,) no-repeat; background-size: 100% 100%; position: absolute; top: 0; left: 0; z-index: 10;}
.item-list .item .img { width: 100%; height: 256rpx; mask-image: url(data:image/png;base64,); -webkit-mask-image: url(data:image/png;base64,); mask-size: 100%; -webkit-mask-size: 100%;}
.item-list .item image{ width: 100%; display: block;}
.item-list .item .title { position: relative;}
.item-list .item .title image { width: 100%;}
.item-list .item .title .name { width: 100%; box-sizing: border-box; height: 110rpx; font-size: 27rpx; color: #ffffff; display: flex; align-items: center; justify-content: space-between; padding:0 40rpx; position: absolute; top: 0; left: 0;}


.footer-tab{ width: 100%; height: 160rpx; background-color: #ffffff; display: flex; position: fixed; left: 0; bottom: 0; z-index: 500;box-shadow: 0 -30rpx 40rpx rgba(0,0,0,0.1);}
.footer-tab view{ box-sizing: border-box; flex: 1; padding-bottom: 30rpx; display: flex; justify-content: center; align-items: center; font-size: 34rpx;}
.footer-tab view.active{ background-color: #002aac; color: #fff;}
.order-body{ margin-bottom: 200rpx;}

.result-frame{ margin: 50rpx;}
.result-frame .icons{ font-size: 200rpx; color: #002aac; display: block; position: relative; margin-bottom: 60rpx;}
.result-frame .icons view { width: 100rpx; height: 120rpx; background-color: #ffffff; position: absolute; top: 30rpx; left: 60rpx; z-index: -1;}
.result-frame .hint-en{ font-size: 100rpx; font-family: 'Helve'; line-height: 0.9;}
.result-frame .hint-cn{ font-size: 72rpx; margin-top: 60rpx;}
.item-list .result-frame { color: #ffffff}
